{
  "name": "unocss-preset-quasar",
  "version": "0.2.2",
  "license": "MIT",
  "author": "Stefan van Herwijnen",
  "description": "UnoCSS preset for Quasar Framework",
  "type": "module",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/index.js"
    },
    "./styles": {
      "types": "./dist/types/styles/index.d.ts",
      "import": "./dist/styles/index.js"
    },
    "./theme": {
      "types": "./dist/types/theme.d.ts",
      "import": "./dist/theme.js"
    }
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/simsustech/unocss-preset-quasar.git",
    "directory": "packages/vitrify"
  },
  "bugs": {
    "url": "https://github.com/simsustech/unocss-preset-quasar/issues"
  },
  "homepage": "https://github.com/simsustech/unocss-preset-quasar/tree/main/#readme",
  "scripts": {
    "build": "tsc",
    "lint": "eslint --ext .ts src",
    "lint:fix": "eslint--ext .ts src --fix",
    "test": "vitest test/"
  },
  "dependencies": {
    "@material/material-color-utilities": "^0.3.0",
    "@unocss/core": "^66.1.1",
    "@unocss/preset-uno": "^66.1.1",
    "@unocss/preset-web-fonts": "66.1.1",
    "@unocss/preset-wind": "^66.1.1",
    "animated-unocss": "^0.0.6",
    "unocss": "66.1.1"
  },
  "devDependencies": {
    "@iconify-json/mdi": "^1.2.3",
    "@quasar/extras": "^1.16.17",
    "@quasar/quasar-ui-qmarkdown": "^2.0.5",
    "@quasar/quasar-ui-qmediaplayer": "^2.0.0-beta.0",
    "@types/node": "^22.15.17",
    "@unocss/preset-icons": "^66.1.1",
    "@unocss/preset-mini": "66.1.1",
    "quasar": "^2.18.1",
    "typescript": "^5.8.3"
  },
  "peerDependencies": {
    "quasar": "^2.18.1"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "files": [
    "dist",
    "src",
    "!dist/**/*.test.js"
  ],
  "keywords": [
    "vite",
    "full stack",
    "fastify"
  ]
}
